{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"javaInterfaces": ["java.io.Serializable"],
	"properties": {
		"STBRegistration": {
			"type": "object",
			"javaInterfaces": ["java.io.Serializable"],
			"properties": {
				"AccountNumber": {
					"type": "string",
					"maxLength": 200
				},
				"MACAddress": {
					"type": "string",
					"maxLength": 20
				},
				"IPAddress": {
					"type": "string",
					"maxLength": 20
				},
				"SerialNumber": {
					"type": "string",
					"maxLength": 20
				},
				"MasterPIN": {
					"type": "integer",
					"minimum": 0,
					"maximum": 99999999
				},
				"Password": {
					"type": "string",
					"maxLength": 50
				},
				"HardwareVersion": {
					"type": "string",
					"maxLength": 30
				},
				"SoftwareVersion": {
					"type": "string",
					"maxLength": 50
				},
				"UIVersion": {
					"type": "string",
					"maxLength": 50
				},
				"CASDeviceId": {
					"type": "string",
					"maxLength": 30
				},
				"STBName": {
					"type": "string",
					"maxLength": 30
				},
				"SupportedModes": {
					"type": "string",
					"maxLength": 50
				},
				"VMXClientId": {
					"type": "string",
					"maxLength": 30
				}
			},
			"additionalProperties": false,
			"required": [
				"AccountNumber",
				"MACAddress",
				"IPAddress",
				"SerialNumber",
				"HardwareVersion",
				"SupportedModes"				
			]
		}
	},
	"additionalProperties": false,
	"required": [
		"STBRegistration"
	]
}

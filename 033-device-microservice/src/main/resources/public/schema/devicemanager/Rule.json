{
   "$schema": "http://json-schema.org/draft-04/schema#",
   "type": "object",
   "javaInterfaces": ["java.io.Serializable"],
   "properties": {
      "Required": {
         "type": "boolean"
      },
      "Length": {
         "type": "integer",
         "minimum": 1,
		 "maximum": 20
      },
      "Numeric": {
         "type": "boolean"
      },
      "Format": {
         "type": "string",
	 "maxLength": 30
      },
      "Unique": {
         "type": "boolean"
      }
   },
   "additionalProperties": false,
   "required": [
      "Required",
      "Numeric",
      "Unique"
   ]
}
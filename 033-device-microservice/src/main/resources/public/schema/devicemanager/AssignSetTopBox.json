{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"javaInterfaces": ["java.io.Serializable"],
	"properties": {
		"STBAssignmentDetails": {
			"type": "array",
			"items": {
				"type": "object",
				"javaInterfaces": ["java.io.Serializable"],
				"properties": {
					"AccountNumber": {
						"type": "string",
						"maxLength": 200
					},
					"MACAddress": {
						"type": "string",
						"maxLength": 20
					},
					"EquipmentId": {
						"type": "integer",
						"minimum": 1,
						"maximum": 999999999
					}
				},
				"additionalProperties": false,
				"required": [
					"AccountNumber"
				]
			}
		}
	},
	"additionalProperties": false,
	"required": [
		"STBAssignmentDetails"
	]
}

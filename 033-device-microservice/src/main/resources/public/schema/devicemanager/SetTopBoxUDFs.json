{
   "$schema": "http://json-schema.org/draft-04/schema#",
   "type": "array",
   "items": {
      "type": "object",
      "javaInterfaces": ["java.io.Serializable"],
      "properties": {
         "Name": {
            "type": "string",
	    "maxLength": 30
         },
         "Value": {
            "type": "string",
	    "maxLength": 255
         }
      },
         "additionalProperties": false,
	   "required": [
	      "Name",
	      "Value"
	   ]
   }
}